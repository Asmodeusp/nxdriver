<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:background="@color/bg"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/common_toolbar"></include>


    <RelativeLayout
        android:id="@+id/rlduizhang"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px104dp">

        <ImageView
            android:id="@+id/rz"
            android:layout_marginLeft="@dimen/px20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/ico_mysetment_duizhang"
            android:layout_width="@dimen/px40dp"
            android:layout_height="@dimen/px40dp">

        </ImageView>
        <TextView
            android:layout_toRightOf="@+id/rz"
            android:layout_marginLeft="@dimen/px10dp"
            android:textSize="@dimen/px30dp"
            android:layout_centerVertical="true"
            android:textColor="@color/color_black"
            android:text="对账管理"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/px30dp"
                android:id="@+id/tvidentificstatus"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginRight="@dimen/px20dp"
                android:src="@drawable/ico_right" />
        </LinearLayout>

    </RelativeLayout>
    <include layout="@layout/common_line"></include>

    <RelativeLayout
        android:id="@+id/rlcount"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px104dp">

        <ImageView
            android:id="@+id/count"
            android:layout_marginLeft="@dimen/px20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/ico_mysetment_jiesuan"
            android:layout_width="@dimen/px40dp"
            android:layout_height="@dimen/px40dp">

        </ImageView>
        <TextView
            android:layout_toRightOf="@+id/count"
            android:layout_marginLeft="@dimen/px10dp"
            android:textSize="@dimen/px30dp"
            android:layout_centerVertical="true"
            android:textColor="@color/color_black"
            android:text="结算管理"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/px30dp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginRight="@dimen/px20dp"
                android:src="@drawable/ico_right" />
        </LinearLayout>

    </RelativeLayout>
    <include layout="@layout/common_line"></include>
    <RelativeLayout
        android:id="@+id/rlfapiao"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px104dp">

        <ImageView
            android:id="@+id/fp"
            android:layout_marginLeft="@dimen/px20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/ico_account_fp"
            android:layout_width="@dimen/px40dp"
            android:layout_height="@dimen/px40dp">

        </ImageView>
        <TextView
            android:layout_toRightOf="@+id/fp"
            android:layout_marginLeft="@dimen/px10dp"
            android:textSize="@dimen/px30dp"
            android:layout_centerVertical="true"
            android:textColor="@color/color_black"
            android:text="发票管理"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/px30dp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginRight="@dimen/px20dp"
                android:src="@drawable/ico_right" />
        </LinearLayout>

    </RelativeLayout>
    <include layout="@layout/common_line"></include>

    <RelativeLayout
        android:id="@+id/rlkoukuan"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px104dp">

        <ImageView
            android:id="@+id/kk"
            android:layout_marginLeft="@dimen/px20dp"
            android:layout_centerVertical="true"
            android:src="@drawable/ico_mysetment_kk"
            android:layout_width="@dimen/px40dp"
            android:layout_height="@dimen/px40dp">

        </ImageView>
        <TextView
            android:layout_toRightOf="@+id/kk"
            android:layout_marginLeft="@dimen/px10dp"
            android:textSize="@dimen/px30dp"
            android:layout_centerVertical="true"
            android:textColor="@color/color_black"
            android:text="扣款管理"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:textSize="@dimen/px30dp"
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginRight="@dimen/px20dp"
                android:src="@drawable/ico_right" />
        </LinearLayout>

    </RelativeLayout>
    <include layout="@layout/common_line"></include>

</LinearLayout>