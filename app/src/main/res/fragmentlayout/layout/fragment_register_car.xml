                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="该车辆是否有资质:"
                    android:textSize="@dimen/px30dp" />

                <com.saimawzc.freight.weight.SwitchButton
                    app:sb_checked="true"
                    app:sb_show_indicator="false"
                    app:sb_checked_color="@color/blue"
                    android:id="@+id/togglequalifications"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/px20dp"
                    android:background="@null" />
            </LinearLayout>

          <include layout="@layout/common_line"></include>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="允许车辆被其他用户添加:"
                    android:textSize="@dimen/px30dp" />

                <com.saimawzc.freight.weight.SwitchButton
                    app:sb_checked_color="@color/blue"
                   app:sb_show_indicator="false"
                    android:id="@+id/toggleadd"
                    app:sb_checked="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/px20dp"
                    android:background="@null" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="是否危化品车型:"
                    android:textSize="@dimen/px30dp" />

                <com.saimawzc.freight.weight.SwitchButton
                    app:sb_checked_color="@color/blue"
                    app:sb_show_indicator="false"
                    android:id="@+id/toggdangerous"
                    app:sb_checked="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/px20dp"
                    android:background="@null" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>
            <RelativeLayout
                android:id="@+id/rlcarNo"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:layout_centerVertical="true"
                    android:id="@+id/tt"
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_toRightOf="@+id/tt"
                    android:id="@+id/car"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="车号:"
                    android:textSize="@dimen/px30dp" />

                <LinearLayout

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:gravity="center_vertical|right"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:visibility="gone"
                        android:padding="@dimen/px10dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:background="@drawable/btn_sold_wihie"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/tvplate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="京"
                            android:textSize="@dimen/px30dp" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:text="∨:"
                            android:textSize="@dimen/px30dp" />
                    </LinearLayout>

                    <EditText
                        android:singleLine="true"
                        android:id="@+id/edcarnum"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/px20dp"
                        android:layout_marginRight="@dimen/px20dp"
                        android:background="@null"
                        android:gravity="center_vertical|right"
                        android:hint="输入车牌号"
                        android:maxLines="1" />
                </LinearLayout>


            </RelativeLayout>

            <include layout="@layout/common_line"></include>

            <LinearLayout
