                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="车牌颜色:"
                    android:textSize="@dimen/px30dp" />

                <RadioGroup
                    android:gravity="center"
                    android:id="@+id/group"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <RadioButton
                        android:paddingLeft="@dimen/px20dp"
                        android:button="@drawable/bg_radiobutton"
                        android:checked="false"
                        android:id="@+id/groupblue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="蓝色" />

                    <RadioButton
                        android:checked="true"
                        android:paddingLeft="@dimen/px20dp"
                        android:drawablePadding="@dimen/px20dp"
                        android:button="@drawable/bg_radiobutton"
                        android:id="@+id/groupyellow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/px20dp"
                        android:layout_marginRight="@dimen/px20dp"
                        android:layout_weight="1"
                        android:text="黄色" />

                </RadioGroup>
            </LinearLayout>

            <include layout="@layout/common_line"></include>


            <RelativeLayout
                android:paddingRight="@dimen/px10dp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_marginRight="@dimen/px20dp"
                    android:layout_toLeftOf="@+id/ico"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textSize="@dimen/px30dp"
                        android:textColor="@color/red"
                        android:text="*"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                    </TextView>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="车型:"
                        android:textSize="@dimen/px30dp" />
                    <TextView
                        android:id="@+id/carmodel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/px20dp"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请选择"
                        android:textSize="@dimen/px30dp" />


                </LinearLayout>
                <ImageView
                    android:id="@+id/ico"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ico_right"
                    android:layout_width="@dimen/px40dp"
                    android:layout_height="@dimen/px40dp" />

            </RelativeLayout>
            <include layout="@layout/common_line"></include>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车辆长度:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:text=""
                    android:layout_weight="1"
                    android:paddingRight="@dimen/px20dp"
                    android:inputType="numberDecimal"
                    android:id="@+id/edCarLength"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
                <TextView
                    android:layout_marginRight="@dimen/px20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="米"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>
            <include layout="@layout/common_line"></include>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车辆宽度:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:layout_weight="1"
                    android:paddingRight="@dimen/px20dp"
                    android:inputType="numberDecimal"
                    android:id="@+id/edCarWith"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
                <TextView
                    android:layout_marginRight="@dimen/px20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="米"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>
            <include layout="@layout/common_line"></include>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车辆高度:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:id="@+id/edCarHeight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:inputType="numberDecimal"
                    android:paddingRight="@dimen/px20dp"
                    android:textSize="@dimen/px30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/px20dp"
                    android:text="米"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="载重:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:inputType="numberDecimal"
                    android:layout_weight="1"
                    android:paddingRight="@dimen/px20dp"
                    android:id="@+id/carweight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
                <TextView
                    android:layout_marginRight="@dimen/px20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="吨"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>

            <RelativeLayout
                android:paddingRight="@dimen/px10dp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_marginRight="@dimen/px20dp"
                    android:layout_toLeftOf="@+id/ico1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="车型品牌:"
                        android:textSize="@dimen/px30dp" />
                    <TextView
                        android:id="@+id/tvcarbrand"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/px20dp"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请选择"
                        android:textSize="@dimen/px30dp" />


                </LinearLayout>
                <ImageView
                    android:id="@+id/ico1"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ico_right"
                    android:layout_width="@dimen/px40dp"
                    android:layout_height="@dimen/px40dp" />

            </RelativeLayout>

            <include layout="@layout/common_line"></include>

            <LinearLayout
                android:id="@+id/rlyuname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="业户名称:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:paddingRight="@dimen/px20dp"
                    android:id="@+id/edusername"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>

            <LinearLayout
                android:id="@+id/llysz"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="运输证号:"
                    android:textSize="@dimen/px30dp" />

                <EditText
                    android:layout_marginRight="@dimen/px20dp"
                    android:paddingRight="@dimen/px20dp"
                    android:id="@+id/edPassNum"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
                android:layout_marginBottom="@dimen/px10dp"
                android:layout_marginLeft="@dimen/px20dp"
                android:layout_marginTop="@dimen/px20dp"
                android:gravity="center"
                android:orientation="horizontal">
                <TextView
                    android:textSize="@dimen/px30dp"
                    android:textColor="@color/red"
                    android:text="*"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="行驶证注册日期:"
                    android:textSize="@dimen/px30dp" />

                <TextView
                    android:layout_marginRight="@dimen/px20dp"
                    android:paddingRight="@dimen/px20dp"
                    android:id="@+id/edxszdata"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/px20dp"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="请输入"
                    android:textSize="@dimen/px30dp" />
            </LinearLayout>

            <include layout="@layout/common_line"></include>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/px80dp"
